{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": ["error", { "js": "ignorePackages" }],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
